IMPLEMENTATION
    Type_i

REFINES
    Type

SEES
    Context, Price

CONCRETE_VARIABLES
    type_i

INVARIANT
    type_i : BIKE --> TYPE &
    types = type_i |>> { 0 }

INITIALISATION
    type_i := BIKE * { 0 }

OPERATIONS

    set_type ( bb , tt ) =
        BEGIN
            type_i ( bb ) := tt
        END;

    qq <-- query_type ( bb ) =
        BEGIN
            qq := type_i ( bb )
        END

END
